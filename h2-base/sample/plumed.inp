UNITS LENGTH=A TIME=fs ENERGY=kcal/mol

COORDINATION GROUPA=1 GROUPB=2 D_0=0.25 R_0=0.50 NN=1 LABEL=nm

HISTOGRAM ...
  ARG=nm
  GRID_MIN=0.0
  GRID_MAX=1.0
  GRID_BIN=2000
  BANDWIDTH=0.005
  LABEL=targetdist
... HISTOGRAM

DUMPGRID GRID=targetdist FILE=histo STRIDE=500000

FLUSH STRIDE=100000
PRINT ARG=nm STRIDE=50000 FILE=colvar

